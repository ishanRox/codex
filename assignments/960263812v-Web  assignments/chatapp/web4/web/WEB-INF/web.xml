<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>B</servlet-name>
        <servlet-class>B</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signin</servlet-name>
        <servlet-class>controller.signin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet1</servlet-name>
        <servlet-class>controller.NewServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loaduser</servlet-name>
        <servlet-class>controller.loaduser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>savemessege</servlet-name>
        <servlet-class>controller.savemessege</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewchat</servlet-name>
        <servlet-class>controller.viewchat</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>B</servlet-name>
        <url-pattern>/B</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signin</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet1</servlet-name>
        <url-pattern>/NewServlet1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loaduser</servlet-name>
        <url-pattern>/loaduser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>savemessege</servlet-name>
        <url-pattern>/savemessege</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewchat</servlet-name>
        <url-pattern>/viewchat</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
